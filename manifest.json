{
  "display_information": {
    "name": "JiraGPT"
  },
  "features": {
    "app_home": {
      "home_tab_enabled": true,
      "messages_tab_enabled": false,
      "messages_tab_read_only_enabled": false
    },
    "bot_user": {
      "display_name": "JiraGPT",
      "always_online": true
    },
    "slash_commands": [
      {
        "command": "/groom",
        "description": "Grooms a Jira ticket",
        "usage_hint": "DEE-1234678",
        "should_escape": false
      },
      {
        "command": "/update",
        "description": "Updates the jira ticket with comment",
        "usage_hint": "DEE-12356789 Added a new bot",
        "should_escape": false
      },
      {
        "command": "/find",
        "description": "Find information for documents",
        "usage_hint": "what is SPAship",
        "should_escape": false
      },
      {
        "command": "/echo",
        "description": "The echo command simply echoes on command",
        "usage_hint": "Hello World",
        "should_escape": false
      }
    ]
  },
  "oauth_config": {
    "scopes": {
      "bot": [
        "app_mentions:read",
        "channels:history",
        "chat:write",
        "commands",
        "groups:history",
        "im:history",
        "im:read",
        "im:write",
        "mpim:history",
        "chat:write.public",
        "users:read.email",
        "users:read"
      ]
    }
  },
  "settings": {
    "event_subscriptions": {
      "bot_events": [
        "app_mention",
        "message.channels",
        "message.groups",
        "message.im",
        "message.mpim"
      ]
    },
    "interactivity": {
      "is_enabled": true
    },
    "org_deploy_enabled": true,
    "socket_mode_enabled": true,
    "token_rotation_enabled": false
  }
}
